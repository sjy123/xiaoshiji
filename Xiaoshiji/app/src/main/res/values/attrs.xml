    <!--<declare-styleable name="MaterialEditText">&lt;!&ndash; The base color of the line and the texts. Default is black. &ndash;&gt;<attr name="baseColor" format="color"/>&lt;!&ndash; The highlight color of the line, and the floating label if the highlightFloatingLabel is true. &ndash;&gt;<attr name="primaryColor" format="color"/>&lt;!&ndash; How the floating label should be shown. Default is none &ndash;&gt;<attr name="floatingLabel"><enum name="none" value="0"/><enum name="normal" value="1"/><enum name="highlight" value="2"/></attr>&lt;!&ndash; The color for when something is wrong.(e.g. exceeding max characters) &ndash;&gt;<attr name="errorColor" format="color"/>&lt;!&ndash; Min characters count limit. 0 means no limit. &ndash;&gt;<attr name="minCharacters" format="integer"/>&lt;!&ndash; max Characters count limit. 0 means no limit. &ndash;&gt;<attr name="maxCharacters" format="integer"/>&lt;!&ndash; Whether to show the bottom ellipsis in singleLine mode &ndash;&gt;<attr name="singleLineEllipsis" format="boolean"/>&lt;!&ndash; Reserved bottom text lines count, no matter if there is some helper/error text. &ndash;&gt;<attr name="minBottomTextLines" format="integer"/>&lt;!&ndash; Helper text at the bottom &ndash;&gt;<attr name="helperText" format="string"/>&lt;!&ndash; Helper text color &ndash;&gt;<attr name="helperTextColor" format="color"/>&lt;!&ndash; Font of the paint used to draw accent texts &ndash;&gt;<attr name="accentTypeface" format="string"/>&lt;!&ndash; Font used on view &ndash;&gt;<attr name="typeface" format="string"/>&lt;!&ndash; Custom float label text &ndash;&gt;<attr name="floatingLabelText" format="string"/>&lt;!&ndash; spacing between the main text and the floating label &ndash;&gt;<attr name="floatingLabelSpacing" format="dimension"/>&lt;!&ndash; Hide underline &ndash;&gt;<attr name="hideUnderline" format="boolean"/>&lt;!&ndash; Auto validate &ndash;&gt;<attr name="autoValidate" format="boolean"/>&lt;!&ndash; Left icon &ndash;&gt;<attr name="iconLeft" format="reference"/>&lt;!&ndash; Right icon &ndash;&gt;<attr name="iconRight" format="reference"/>&lt;!&ndash; Padding between icon(s) and the main area. 8dp by default. &ndash;&gt;<attr name="iconPadding" format="dimension"/>&lt;!&ndash; The floating label's text size. 12sp by default. &ndash;&gt;<attr name="floatingLabelTextSize" format="dimension"/>&lt;!&ndash; The bottom texts' size. 12sp by default. &ndash;&gt;<attr name="bottomTextSize" format="dimension"/>&lt;!&ndash; Always show the floating label, instead of animating it in/out. &ndash;&gt;<attr name="floatingLabelAlwaysShown" format="boolean"/>&lt;!&ndash; Always show the helper text, no matter if the edit text is focused. &ndash;&gt;<attr name="helperTextAlwaysShown" format="boolean"/></declare-styleable>-->
